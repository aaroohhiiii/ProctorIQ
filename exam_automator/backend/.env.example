# ProctorIQ Backend Environment Configuration

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Pinecone Configuration (Optional - for vector database)
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=your_pinecone_environment_here
PINECONE_INDEX_NAME=proctoriq-index

# Application Configuration
DEBUG=True
APP_NAME=ProctorIQ API
APP_VERSION=1.0.0

# File Upload Configuration
MAX_FILE_SIZE=52428800  # 50MB in bytes
UPLOAD_DIRECTORY=uploads

# CORS Configuration (for frontend)
CORS_ORIGINS=["http://localhost:3000", "http://127.0.0.1:3000"]

# Database URL (for future use)
# DATABASE_URL=sqlite:///./proctoriq.db
